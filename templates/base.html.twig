<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>{% block title %}Smarts Leads!{% endblock %}</title>
    {% block stylesheets %}
        <!--    font-->
        <link href="https://fonts.googleapis.com/css?family=Quicksand|Roboto:300,400,500,700" rel="stylesheet">
        <!-- font awesome -->
        <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.7.2/css/all.css"
              integrity="sha384-fnmOCqbTlWIlj8LyTjo7mOUStjsKC4pOpQbqyi7RrhN7udi9RwhKkMHpvLbHG9Sr"
              crossorigin="anonymous">
        <!-- Bootstrap CSS -->
        <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.2.1/css/bootstrap.min.css"
              integrity="sha384-GJzZqFGwb1QTTN6wy59ffF1BuGJpLSa9DkKMp0DgiMDm4iYMj70gZWKYbI706tWS"
              crossorigin="anonymous">
        <!--    datatables-->
        <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/1.10.19/css/jquery.dataTables.css">
        <!-- bootstrap.css -->
        <link rel="icon" type="image/png" href="{{ asset("assests/img/favicon.png") }}"/>
        <!-- <link rel="stylesheet" href="css/bootstrap.min.css">  -->
        <link rel="stylesheet" href="{{ asset("assests/css/media.css") }}" type="text/css">
        {% if app.request.attributes.get('_route') == 'dashboard' %}
        <link rel="stylesheet" href="{{ asset("assests/css/dashboard.css") }}">
        {% else %}
        <link rel="stylesheet" href="{{ asset("assests/css/style.css") }}" type="text/css">
        {% endif %}
        {#<link rel="stylesheet" href="{{ asset("assests/css/bootstrap.min.css") }}" type="text/css">#}

    {% endblock %}


</head>
<body>


{% include 'layout/header.html.twig' %}
{% include 'layout/leftbar.html.twig' %}


{% set mycol = render(controller(
    'App\\AdminBundle\\Controller\\ParameterController::colorpart'
    )) %}


<style>
    .list-group a:hover {
        border-left: 5px solid{{ mycol }};
        color: {{ mycol }};
    }

    .list-group-item.active {
        z-index: 2;
        color: #9e9898;
        background-color: transparent;
        border-color: transparent;
        border-left: 5px solid{{ mycol }};
    }

    .pagibutton button {
        float: right;
        margin-right: 25px;
        background-color: {{ mycol }};
        font-size: 13px;
        font-weight: 500;
        transition: 1s ease-in;
        padding: 13px 25px;
        border-color: {{ mycol }};
    }

    .login_button {
        background-color: {{ mycol }};
        color: white;
        padding: 10px 20px;
        border: none;
        cursor: pointer;
        width: 100%;
        opacity: 0.9;
        transition: 1s ease-in;
    }

    .enterprise_contact_td {
        width: 40px;
        background: {{ mycol }};
        color: white;
        font-size: 15px;
        text-align: center;
        margin: 0 auto;
        border-radius: 12px;
        padding: 1px 0px;
    }

    .contact_formnav nav a:hover {
        border-bottom: 3px solid{{ mycol }};

        color: #CF1742;
    }

    .contact_formnav nav .active {
        border-bottom: 3px solid{{ mycol }};

        color: {{ mycol }};
    }

    .contact_userimg_button a:hover {
        color: {{ mycol }};
    }

    .contact_userimg_button a i {
    }

    .contact_userimg_button a i:hover {
        color: {{ mycol }};
    }

    .input_border_left {
        border-left: 4px solid{{ mycol }};
    }

    .enregisterbutton button {
        /*    float: right;*/
        /*    margin-right: 25px;*/
        background-color: {{ mycol }};
        font-size: 13px;
        font-weight: bold;
        transition: 1s ease-in;
        padding: 13px 25px;
        border: 2px solid white;
    }

    .enregisterbutton button:hover {
        background-color: white;
        color: {{ mycol }};
        border: 2px solid{{ mycol }};
    }

    .annulerbutton {
        text-align: right;
    }

    .annulerbutton button {
        /*    float: right;*/
        /*    margin-right: 25px;*/
        background-color: white;
        font-size: 13px;
        font-weight: bold;
        transition: 1s ease-in;
        padding: 13px 50px;
        color: {{ mycol }};
        border: 2px solid{{ mycol }};
    }

    .annulerbutton button:hover {
        background-color: {{ mycol }};
        border: 2px solid white;

    }

    .demandbutton {
        text-align: right;
    }

    .demandbutton button {
        /*    float: right;*/
        /*    margin-right: 25px;*/
        background-color: white;
        font-size: 13px;
        font-weight: bold;
        transition: 1s ease-in;
        padding: 13px 15px;
        color: {{ mycol }};
        border: 2px solid{{ mycol }};
    }

    .demandbutton button:hover {
        background-color: {{ mycol }};
        border: 2px solid white;

    }


    .supprimebutton {
        text-align: right;
    }

    .supprimebutton button {
        /*    float: right;*/
        /*    margin-right: 25px;*/
        background-color: white;
        font-size: 13px;
        font-weight: bold;
        transition: 1s ease-in;
        padding: 13px 45px;
        color: {{ mycol }};
        border: 2px solid{{ mycol }};
    }

    .supprimebutton button:hover {
        background-color: {{ mycol }};
        border: 2px solid white;

    }

    .retourbutton button:hover {
        background-color: white;
        /*    border: 2px solid white;*/
        color: {{ mycol }};
        border: 2px solid{{ mycol }};
        padding: 13px 35px;
    }

    .enterprise3_form_a {
        /*       width: 40px;*/
        background: {{ mycol }};
        color: white;
        font-size: 15px;
        text-align: center;
        margin: 0 auto;
        border-radius: 12px;
        padding: 1px 10px;
    }

    .contact4col1c1span1 {
        color: {{ mycol }};
        font-size: 16px;
        font-weight: 500;
    }

    .annulerbutton3 button {
        /*    float: right;*/
        /*    margin-right: 25px;*/
        background-color: white;
        font-size: 13px;
        font-weight: bold;
        transition: 1s ease-in;
        padding: 13px 15px;
        color: {{ mycol }};
        border: 2px solid{{ mycol }};
    }

    .annulerbutton3 button:hover {
        background-color: {{ mycol }};
        border: 2px solid white;

    }

    @media (min-width: 992px) and (max-width: 1199.98px) {
        .annulerbutton3 button {
            /*    float: right;*/
            /*    margin-right: 25px;*/
            background-color: white;
            font-size: 13px;
            font-weight: bold;
            transition: 1s ease-in;
            padding: 13px 15px;
            color: {{ mycol }};
            border: 2px solid{{ mycol }};
        }

        .annulerbutton3 button:hover {
            background-color: {{ mycol }};
            border: 2px solid white;

        }
    }

    .contact1span {
        font-size: 16px;
        color: {{ mycol }};
    }

    .custom-control-input:checked ~ .custom-control-label::before {
        color: #fff;
        border: none;
        /* border-color: #007bff; */
        background-color: {{ mycol }};
    }

    ul#menu {
        margin-top: 7px;
    }

    ul#menu li {
        display: inline;
    }

    ul#menu li a {
        color: #9E9898;
        padding: 0px 15px;
    }

    ul#menu li a:hover {
        color: {{ mycol }};
    }
</style>

{% block body %}{% endblock %}
{% block javascripts %}
    <script src="https://canvasjs.com/assets/script/canvasjs.min.js"></script>
    <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js"
            integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo"
            crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.6/umd/popper.min.js"
            integrity="sha384-wHAiFfRlMFy6i5SRaxvfOCifBUQy1xHdJ/yoi7FRNXMRBu5WHdZYu1hA6ZOblgut"
            crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.2.1/js/bootstrap.min.js"
            integrity="sha384-B0UglyR+jN6CkvvICOB2joaf5I4l3gm9GU6Hc1og6Ls7i6U/mkkaduKaBhlAXv9k"
            crossorigin="anonymous"></script>
    <!--  data_tables -->
    <script type="text/javascript" charset="utf8"
            src="https://cdn.datatables.net/1.10.19/js/jquery.dataTables.js"></script>

    <script>
        $(document).ready(function () {
            //initialisation du tableau
            $('#myTable').DataTable({
                "lengthMenu": [[10, 25, 50, -1],
                    ["10", "25", "50", "Toutes"]],
                "language": {
                    "paginate": {
                        "previous": '<i class="fa fa-arrow-left"></i>',
                        "next": '<i class="fa fa-arrow-right"></i>'
                    },
                    searchPlaceholder: "Rechercher",
                    search: "",
                    "sSearch": '<a class="btn searchBtn" id="searchBtn"><i class="fa fa-search"></i></a>',
                    sLengthMenu: "_MENU_",
                    infoEmpty: "Aucunes données trouvées",
                    "info": "",
                },
                "dom": '<"top"if>rt<"bottom"lp><"clear">'
            });

            $('#check-all').change(function () {
                if ($(this).is(':checked')) {
                    $('#myTable tbody .custom-checkbox input').prop('checked', true);
                } else {
                    $('#myTable tbody .custom-checkbox input').prop('checked', false);
                }
            });
            $('#delete-select-company').click(function () {
                var company = [];
                $("#myTable tbody .custom-checkbox input").each(function (index) {
                    if ($(this).prop('checked') == true) {
                        company.push($(this).val());
                    }
                });
                if (company.length > 0) {
                    $('#companys').val(company);
                    $("#form-delete-company").submit();
                } else {
                }
            });
            $('#delete-select-contact').click(function () {
                var contact = [];
                $("#myTable tbody .custom-checkbox input").each(function (index) {
                    if ($(this).prop('checked') == true) {
                        contact.push($(this).val());
                    }
                });
                if (contact.length > 0) {
                    $('#contacts').val(contact);
                    $("#form-delete-contact").submit();
                } else {
                }
            });
        });
    </script>
    
{% endblock %}
</body>
</html>
